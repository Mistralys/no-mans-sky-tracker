{
    "name": "mistralys\/no-mans-sky-tracker",
    "description": "PHP-based tool for tracking solar systems and outposts in No Man's Sky.",
    "license": "MIT",
    "type": "tool",
    "authors": [
        {
            "name": "Sebastian Mordziol",
            "email": "s.mordziol@mistralys.eu",
            "role": "Project Lead"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/lipis\/flag-icon-css.git"
        },
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/medialize\/URI.js.git"
        },
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/Mistralys\/deepl-api-connector.git"
        },
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/Mistralys\/mousetrap.git"
        },
        {
            "type": "vcs",
            "url": "https:\/\/github.com\/Mistralys\/simplemde-markdown-editor.git"
        },
        {
            "type": "package",
            "package": {
                "name": "marcj\/css-element-queries",
                "version": "1.2.2",
                "type": "library",
                "source": {
                    "url": "https:\/\/github.com\/marcj\/css-element-queries.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        },
        {
            "type": "package",
            "package": {
                "name": "zenorocha\/clipboardjs",
                "version": "2.0.8",
                "type": "library",
                "source": {
                    "url": "https:\/\/github.com\/zenorocha\/clipboard.js.git",
                    "type": "git",
                    "reference": "master"
                }
            }
        }
    ],
    "autoload": {
        "classmap": [
            "assets\/classes\/",
            "themes\/default\/templates"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests\/assets\/TestClasses"
        ]
    },
    "require": {
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-redis": "*",
        "mistralys\/application_framework": "2.4.4",
        "php": "^7.4"
    },
    "require-dev": {
        "phpstan\/extension-installer": ">=1.1.0",
        "phpstan\/phpstan": ">=1.4",
        "phpstan\/phpstan-phpunit": ">=1.1.1",
        "phpunit\/phpunit": ">=9.5",
        "roave\/security-advisories": "dev-latest"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "phpstan\/extension-installer": true
        }
    }
}